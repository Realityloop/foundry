        "drupal/jsonapi_image_styles": "^3.0",
        "drupal/jsonapi_menu_items": "^1.2",
        "drupal/jsonapi_node_preview_tab": "^1.0",
        "drupal/jsonapi_resources": "^1.2",
        "drupal/jsonapi_user_resources": "dev-project-update-bot-only",
